# english declarations map for gneposis publisher
# by Adam Szieberth

# author and title declarations

AUTHOR : author
TITLE : title

# declarations of metadata

DATE : date
DESCRIPTION : description
ISBN : isbn

# main structure declarations

BOOK : book
PART : part
CHAPTER : chapter
SECTION: section
PARAGRAPH : paragraph
*** : stars

# remaining structure declarations

COPYRIGHT : copyright
DEDICATION : dedication
FOREWORD : foreword
PREFACE : preface
INTRO : intro

ANNOUNCEMENT : announcement

# notes and footnotes

FOOTNOTE : footnote
NOTE : note

# style declarations

LEFT : left
RIGHT : right
CENTER : center

/ : end

# declarations for special contents

VERSE : verse
